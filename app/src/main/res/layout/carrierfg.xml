                        android:textSize="16sp" />
                </RelativeLayout>

                <View
                    android:id="@+id/carrier_info_view2"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_ll_infomation"
                    android:background="@color/divide_line" />

                <TextView
