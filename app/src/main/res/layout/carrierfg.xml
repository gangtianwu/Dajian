                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_address"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view3"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="单位地址"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_address_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view3"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view4"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_address"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_reg_address"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view4"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="注册省市"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_reg_address_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view4"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view5"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_reg_address"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_type"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view5"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="单位类型"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_type_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view5"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view6"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_type"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_master"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view6"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="法人姓名"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_master_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view6"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view7"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_master"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_master_id"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view7"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="身份证号"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_master_id_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view7"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view8"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_master_id"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_master_tel"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view8"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="企业电话"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_master_tel_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view8"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view9"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_master_tel"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_master_fax"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_info_view9"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="企业传真"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_master_fax_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view9"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />
            </RelativeLayout>
        </android.support.v7.widget.CardView>

        <!--企业资质信息-->
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardBackgroundColor="@color/cardview_light_background"
            app:cardCornerRadius="0.5dp"
            app:cardElevation="1dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/carrier_tv_reg_information_ll"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:gravity="center_vertical">

                    <View
                        android:id="@+id/carrierfg_reg_view1"
                        android:layout_width="2dp"
                        android:layout_height="17dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:background="#008AFF" />

                    <TextView
                        android:id="@+id/carrier_tv_reg_information"
                        android:layout_width="wrap_content"
                        android:layout_height="44dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/carrierfg_reg_view1"
                        android:gravity="center"
                        android:text="企业资质信息"
                        android:textColor="@color/mine_info_todo_text_process"
                        android:textSize="16sp" />
                </RelativeLayout>

                <View
                    android:id="@+id/carrierfg_reg_view2"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_reg_information_ll"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_reg_no"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrierfg_reg_view2"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="营业执照"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_reg_no_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrierfg_reg_view2"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrierfg_reg_view3"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_reg_no"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_org_cert_no"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrierfg_reg_view3"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="经营许可证号"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_org_cert_no_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrierfg_reg_view3"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrierfg_reg_view4"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_org_cert_no"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_start_time"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrierfg_reg_view4"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="有效期起"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_start_time_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrierfg_reg_view4"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrierfg_reg_view5"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_start_time"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_end_time"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrierfg_reg_view5"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="有效期止"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_end_time_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrierfg_reg_view5"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

            </RelativeLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>
</ScrollView>