                <TextView
                    android:id="@+id/carrier_tv_name_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_info_view2"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_info_view3"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_name"
