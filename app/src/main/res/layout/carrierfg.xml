<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/nodata_background"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardBackgroundColor="@color/cardview_light_background"
            app:cardCornerRadius="0.5dp"
            app:cardElevation="1dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/carrier_contact_ll"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:gravity="center_vertical">

                    <View
                        android:id="@+id/carrierfg_view1"
                        android:layout_width="2dp"
                        android:layout_height="17dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:background="#008AFF" />

                    <TextView
                        android:id="@+id/carrier_contact_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="44dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/carrierfg_view1"
                        android:gravity="center"
                        android:text="联系人信息"
                        android:textColor="@color/mine_info_todo_text_process"
                        android:textSize="16sp" />
                </RelativeLayout>

                <View
                    android:id="@+id/carrier_contact_view2"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_contact_ll"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_contact"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_contact_view2"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="@string/carrier_contact"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_contact_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_contact_view2"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_contact_view3"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_contact"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_user_id"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_contact_view3"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="@string/carrier_user_id"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_user_id_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_contact_view3"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_contact_view4"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_user_id"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_master_mail"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_contact_view4"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="@string/carrier_master_mail"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_master_mail_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_contact_view4"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

                <View
                    android:id="@+id/carrier_contact_view5"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_below="@+id/carrier_tv_master_mail"
                    android:layout_marginLeft="15dp"
                    android:background="@color/divide_line" />

                <TextView
                    android:id="@+id/carrier_tv_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/carrier_contact_view5"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:text="@string/carrier_mobile"
                    android:textColor="@color/mine_info_todo_text_process"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/carrier_tv_mobile_value"
                    android:layout_width="wrap_content"
                    android:layout_height="44dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/carrier_contact_view5"
                    android:gravity="center"
                    android:paddingRight="15dp"
                    android:text="同意"
                    android:textColor="@color/mine_info_radiogroup_unchecked"
                    android:textSize="16sp" />

            </RelativeLayout>
        </android.support.v7.widget.CardView>

        <!--企业基本信息-->
        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            app:cardBackgroundColor="@color/cardview_light_background"
            app:cardCornerRadius="0.5dp"
            app:cardElevation="1dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/carrier_ll_infomation"
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:gravity="center_vertical">

                    <View
                        android:id="@+id/carrierfg_info_view1"
                        android:layout_width="2dp"
                        android:layout_height="17dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="15dp"
                        android:background="#008AFF" />

                    <TextView
                        android:id="@+id/carrier_tv_infomation"
                        android:layout_width="wrap_content"
                        android:layout_height="44dp"
                        android:layout_marginLeft="10dp"
