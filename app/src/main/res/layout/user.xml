<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/nodata_background"
    >

    <RelativeLayout
        android:background="@color/detail_title"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        >
        <ImageView
            android:layout_centerVertical="true"
            android:id="@+id/car_detail_mainactivity_iv"
            android:src="@mipmap/back"
            android:layout_marginLeft="15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
        <TextView
            android:textColor="@color/mine_info_todo_text_process"
            android:paddingLeft="10dp"
            android:gravity="center"
            android:id="@+id/tv_item"
            android:textSize="18sp"
            android:text="用户信息"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        app:cardBackgroundColor="@color/cardview_light_background"
        app:cardCornerRadius="0.5dp"
        app:cardElevation="1dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/carrier_reg_no"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/carrierfg_reg_view2"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:text="用户ID"
                android:textColor="@color/mine_info_todo_text_process"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/carrier_reg_no_value"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/carrierfg_reg_view2"
                android:gravity="center"
                android:paddingRight="15dp"
                android:text="无(未登录)"
                android:textColor="@color/mine_info_radiogroup_unchecked"
                android:textSize="16sp" />

            <View
                android:id="@+id/carrierfg_reg_view3"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_below="@+id/carrier_reg_no"
                android:layout_marginLeft="15dp"
                android:background="@color/divide_line" />

            <TextView
                android:id="@+id/carrier_tv_org_cert_no"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/carrierfg_reg_view3"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:text="用户类型"
                android:textColor="@color/mine_info_todo_text_process"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/carrier_tv_org_cert_no_value"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/carrierfg_reg_view3"
                android:gravity="center"
                android:paddingRight="15dp"
                android:text="无(未登录)"
                android:textColor="@color/mine_info_radiogroup_unchecked"
                android:textSize="16sp" />

            <View
                android:id="@+id/carrierfg_reg_view4"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_below="@+id/carrier_tv_org_cert_no"
                android:layout_marginLeft="15dp"
                android:background="@color/divide_line" />

            <TextView
                android:id="@+id/carrier_tv_start_time"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/carrierfg_reg_view4"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:text="用户昵称"
                android:textColor="@color/mine_info_todo_text_process"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/carrier_tv_start_time_value"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/carrierfg_reg_view4"
                android:gravity="center"
                android:paddingRight="15dp"
                android:text="无(未登录)"
                android:textColor="@color/mine_info_radiogroup_unchecked"
                android:textSize="16sp" />

            <View
                android:id="@+id/carrierfg_reg_view5"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_below="@+id/carrier_tv_start_time"
                android:layout_marginLeft="15dp"
                android:background="@color/divide_line" />

            <TextView
                android:clickable="true"
                android:id="@+id/carrier_tv_end_time"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/carrierfg_reg_view5"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:text="其他信息"
                android:textColor="@color/mine_info_todo_text_process"
                android:textSize="16sp" />

            <TextView
                android:clickable="true"
                android:id="@+id/carrier_tv_end_time_value"
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/carrierfg_reg_view5"
                android:gravity="center"
                android:paddingRight="15dp"
                android:text="                                                                          "
                android:textColor="@color/mine_info_radiogroup_unchecked"
                android:textSize="16sp" />

        </RelativeLayout>
    </android.support.v7.widget.CardView>
</LinearLayout>